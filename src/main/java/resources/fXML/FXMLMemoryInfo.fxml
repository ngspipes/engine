<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="143.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ngspipesengine.userInterface.controllers.FXMLMemoryInfoController">
   <children>
      <HBox alignment="CENTER" prefHeight="66.0" prefWidth="300.0">
         <children>
            <Label fx:id="memoryInfoLabel" text="Engine will use:" textFill="#3e294a">
               <font>
                  <Font name="Century Gothic Bold" size="16.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lQuantity" text="1000">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Label>
            <Label text="GB">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <Slider fx:id="sQuantity" />
         </children>
      </HBox>
      <Button id="okBtn" fx:id="bOk" mnemonicParsing="false" prefHeight="37.0" prefWidth="38.0" styleClass="buttons">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <stylesheets>
            <URL value="@../css/Buttons.css" />
            <URL value="@../css/OkButton.css" />
         </stylesheets>
      </Button>
   </children>
</VBox>
