<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="518.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ngspipesengine.userInterface.controllers.FXMLConfigurePipelineController">
   <children>
      <VBox VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="resultsLabel" text="Results path:" textFill="#3e294a" VBox.vgrow="ALWAYS">
               <font>
                  <Font name="Century Gothic Bold" size="18.0" />
               </font>
            </Label>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="tFResults" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button id="searchBtn" fx:id="bResults" mnemonicParsing="false" styleClass="buttons" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <stylesheets>
                        <URL value="@../css/Buttons.css" />
                        <URL value="@../css/SearchButton.css" />
                     </stylesheets>
                  </Button>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </VBox.margin>
      </VBox>
      <VBox VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="resultsLabel1" text="Inputs path:" textFill="#3e294a" VBox.vgrow="ALWAYS">
               <font>
                  <Font name="Century Gothic Bold" size="18.0" />
               </font>
            </Label>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <TextField fx:id="tFInputs" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button id="searchBtn" fx:id="bInputs" mnemonicParsing="false" styleClass="buttons" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <stylesheets>
                        <URL value="@../css/SearchButton.css" />
                        <URL value="@../css/Buttons.css" />
                     </stylesheets>
                  </Button>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER_LEFT">
         <children>
            <Label text="Engine:" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
            <ComboBox fx:id="cBEngines" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="20.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="From">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="cBFrom" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ComboBox>
            <Label text="To">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="cBTo" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="memoryInfoLabel" text="Engine will use:" textFill="#3e294a">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lMemoryQuantity" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="1000">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label text="MB">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Slider fx:id="sMemoryQuantity">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <ImageView fx:id="iVMemoryWarning" fitHeight="40.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
               <image>
                  <Image url="@../images/warning.jpg" />
               </image>
            </ImageView>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="memoryInfoLabel1" text="Engine will use:" textFill="#3e294a">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="lProcessorsQuantity" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="2">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label text="Processors" />
            <Slider fx:id="sProcessorsQuantity">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
      <Button id="okBtn" fx:id="bOk" mnemonicParsing="false" prefHeight="33.0" prefWidth="31.0" styleClass="buttons" VBox.vgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <stylesheets>
            <URL value="@../css/OkButton.css" />
            <URL value="@../css/Buttons.css" />
         </stylesheets>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
